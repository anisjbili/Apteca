<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/anis"
    tools:context="com.example.asus.an_teca.About_Activity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/anis"
        tools:context="com.example.asus.an_teca.Liste_Pharmacie">

        <Button
            android:id="@+id/button3"
            android:layout_width="50dp"
            android:layout_height="55dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="148dp"
            android:layout_marginStart="148dp"
            android:layout_marginTop="136dp"
            android:background="@drawable/home" />

        <Button
            android:id="@+id/button"
            android:layout_width="50dp"
            android:layout_height="55dp"
            android:layout_alignTop="@+id/button3"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:layout_toEndOf="@+id/button3"
            android:layout_toRightOf="@+id/button3"
            android:background="@drawable/comment" />

        <Button
            android:id="@+id/button4"
            android:layout_width="50dp"
            android:layout_height="55dp"
            android:layout_alignTop="@+id/button"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:layout_toEndOf="@+id/button"
            android:layout_toRightOf="@+id/button"
            android:background="@drawable/parametres" />

        <Button
            android:id="@+id/button5"
            android:layout_width="55dp"
            android:layout_height="60dp"
            android:layout_alignBottom="@+id/button3"
            android:layout_marginEnd="61dp"
            android:layout_marginRight="61dp"
            android:layout_toLeftOf="@+id/button"
            android:layout_toStartOf="@+id/button"
            android:background="@drawable/news" />

        <Button
            android:id="@+id/button6"
            android:layout_width="50dp"
            android:layout_height="55dp"
            android:layout_alignBottom="@+id/button5"
            android:layout_marginEnd="14dp"
            android:layout_marginRight="14dp"
            android:layout_toLeftOf="@+id/button5"
            android:layout_toStartOf="@+id/button5"
            android:background="@drawable/produit" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="209dp"
            android:text="Contact Us"
            android:textAlignment="center"
            android:textSize="20sp"
            android:textStyle="bold" />
        <Button
            android:id="@+id/left"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignTop="@+id/textView2"
            android:layout_toLeftOf="@+id/textView2"
            android:layout_toStartOf="@+id/textView2"
            android:background="@drawable/left" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:text="Anais parapharmacie, El ghazela
            Ariana, Tunisia"
            android:textColor="@android:color/background_dark"
            android:textSize="20sp" />
        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:layout_alignLeft="@+id/imageView3"
            android:layout_alignStart="@+id/imageView3"
            android:layout_alignTop="@+id/textView4"
            app:srcCompat="@drawable/direction" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="300dp"
            android:layout_height="45dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/textView4"
            android:layout_marginTop="19dp"
            android:gravity="center_vertical"
            android:text="anaispharma"
            android:textColor="@android:color/background_dark"
            android:textSize="20sp" />


        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:layout_alignTop="@+id/textView5"
            android:layout_marginEnd="9dp"
            android:layout_toStartOf="@+id/textView5"
            app:srcCompat="@drawable/messenger" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/textView5"
            android:layout_marginTop="14dp"
            android:text="Call 70935808"
            android:gravity="center_vertical"
            android:textColor="@android:color/background_dark"
            android:textSize="20sp" />
        <ImageView
            android:id="@+id/tel"
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:layout_alignTop="@+id/textView6"
            android:layout_marginEnd="9dp"
            android:layout_toStartOf="@+id/textView6"
            app:srcCompat="@drawable/phone" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/textView6"
            android:layout_marginTop="17dp"
            android:gravity="center_vertical"
            android:text="8:30AM - 8:30PM"
            android:textColor="@android:color/background_dark"
            android:textSize="20sp" />


    </RelativeLayout>
</RelativeLayout>